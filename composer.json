{
    "name": "lee-to/laravel-model-localization",
    "description": "Laravel model localization",
    "keywords": ["laravel", "localization"],
    "type": "library",
    "homepage": "https://github.com/lee-to/laravel-model-localization",
    "require": {
        "php": "^7.3|^8.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "mockery/mockery": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Danil Shutsky",
            "email": "leetodev@ya.ru",
            "homepage": "https://github.com/lee-to"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Leeto\\Localization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leeto\\Localization\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leeto\\Localization\\Providers\\LocalizationServiceProvider"
            ]
        }
    }
}
